{
  "name": "niklesh/health-check-bundle",
  "description": "Health check bundle",
  "type": "symfony-bundle",
  "license": "proprietary",
  "autoload": {
    "psr-4": {
      "niklesh\\HealthCheckBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "niklesh\\HealthCheckBundle\\Tests\\": "tests"
    }
  },
  "require": {
    "php": "^7.1",
    "symfony/config": "^4.1",
    "symfony/flex": "^1.0",
    "symfony/framework-bundle": "^4.1",
    "symfony/monolog-bundle": "^3.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.2",
    "phpmd/phpmd": "^2.6",
    "sebastian/phpcpd": "^4.0",
    "squizlabs/php_codesniffer": "^3.1",
    "phing/phing": "^2.16",
    "phpstan/phpstan": "^0.9.2",
    "lookyman/phpstan-symfony": "^0.5.0",
    "phpstan/phpstan-phpunit": "^0.9.4",
    "sensiolabs/security-checker": "^4.1",
    "phpstan/phpstan-doctrine": "^0.9.1"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd",
      "security-checker security:check": "script"
    }
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "id": "01C2BB7VK9WQJPQMHTK399MP3S",
      "allow-contrib": false
    }
  }
}
